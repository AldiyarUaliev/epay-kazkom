{
    "name": "dosarkz/epay-kazcom",
    "description": "Payment service from Kazkommertsbank",
    "type": "library",
    "authors": [
        {
            "name": "Yerzhan Ashenov",
            "email": "ashenov.e@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
